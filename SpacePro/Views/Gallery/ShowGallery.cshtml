@model IEnumerable<Entities.Bodies.BodyImage>
 <!-- PAGE-HEADER -->
<div class="page-header">
    <h1 class="page-title">Gallery</h1>
    <div>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/Home/Index">Home</a></li>
            <li class="breadcrumb-item active" aria-current="page">Gallery</li>
        </ol>
    </div>
</div>
<!-- PAGE-HEADER END -->
<!-- GALLERY DEMO OPEN -->
<div class="masonry row">
    @foreach (var bi in Model)
    {
        <div class="col-xl-3 col-lg-4 col-sm-6">
            <div class="brick">
                <a href="@bi.Url" class="js-img-viewer" data-caption="IMAGE-01"
                   data-id="lion">
                    <img src="@bi.Url" alt="@bi.AlternativeText" />
                </a>
            </div>
        </div>
    }
</div>
<!-- GALLERY DEMO CLOSED -->
<!-- PAGINATION OPEN -->
<div class=" row mb-5">
    <div class=" col-md-6 mt-1 d-none d-md-block">1 - 10 of 234 photos</div>
    <div class=" col-md-6">
        <div class=" float-end">
            <ul class=" pagination">
                <li class=" page-item page-prev disabled">
                    <a class=" page-link" href="#">Prev</a>
                </li>
                <li class=" page-item active"><a class=" page-link" href=" #">1</a></li>
                <li class=" page-item"><a class=" page-link" href=" #">2</a></li>
                <li class=" page-item"><a class=" page-link" href=" #">3</a></li>
                <li class=" page-item"><a class=" page-link" href=" #">4</a></li>
                <li class=" page-item"><a class=" page-link" href=" #">5</a></li>
                <li class=" page-item page-next">
                    <a class=" page-link" href=" #">Next</a>
                </li>
            </ul>
        </div>
    </div>
    <!-- COL-END -->
</div>